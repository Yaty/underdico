{
  "name": "underdico",
  "version": "1.0.0",
  "description": "Underdico",
  "license": "MIT",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --forceExit --config ./test/jest-e2e.json",
    "tag": "sudo docker build -t eu.gcr.io/underdico/underdico .",
    "push": "sudo docker push eu.gcr.io/underdico/underdico",
    "set-image": "kubectl set image deployment underdico underdico=$(sudo docker inspect --format='{{index .RepoDigests 0}}' eu.gcr.io/underdico/underdico:latest)",
    "deploy": "npm run tag && npm run push && npm run set-image"
  },
  "dependencies": {
    "@google-cloud/storage": "^3.0.2",
    "@nestjs/common": "^6.5.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/platform-socket.io": "^6.5.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/websockets": "^6.5.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "config": "^3.2.0",
    "iso-639-1": "^2.0.5",
    "mongoose": "^5.6.4",
    "morphism": "^1.12.0",
    "nestjs-redis": "^1.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pjson": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.0.7",
    "typegoose": "^5.7.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.2",
    "@types/config": "^0.0.34",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^4.0.13",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.8",
    "@types/node": "^12.6.2",
    "@types/p-event": "^3.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "p-event": "^4.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "socket.io-client": "^2.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
